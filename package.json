{
  "name": "nuxt-app",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "migrate": "prisma migrate dev --name",
    "db:gui": "prisma studio",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "start": "node .output/server/index.mjs",
    "deps": "npx taze major",
    "lint:js": "eslint --ext \".ts,.vue\" --ignore-path .gitignore .",
    "lint:prettier": "prettier --check .",
    "lint": "pnpm lint:js && pnpm lint:prettier",
    "lintfix": "prettier --write --list-different . && pnpm lint:js --fix",
    "e2e:test": "cypress run",
    "test": "vitest run"
  },
  "devDependencies": {
    "@nuxt/devtools": "latest",
    "@nuxt/test-utils": "^3.6.2",
    "@nuxtjs/color-mode": "^3.3.0",
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "@nuxtjs/i18n": "8.0.0-beta.12",
    "@nuxtjs/tailwindcss": "^6.8.0",
    "@types/node": "^20.4.0",
    "@typescript-eslint/parser": "^5.61.0",
    "cypress": "^12.16.0",
    "daisyui": "^3.1.9",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.15.1",
    "nuxt": "^3.6.2",
    "nuxt-icon": "^0.4.2",
    "prettier": "^2.8.8",
    "prisma": "^4.16.2",
    "sass": "^1.63.6",
    "taze": "^0.11.2",
    "typescript": "^5.1.6",
    "vitest": "^0.32.0"
  },
  "dependencies": {
    "@bytemd/plugin-breaks": "^1.21.0",
    "@bytemd/plugin-frontmatter": "^1.21.0",
    "@bytemd/plugin-gfm": "^1.21.0",
    "@pinia/nuxt": "^0.4.11",
    "@prisma/client": "4.16.2",
    "@tailwindcss/typography": "^0.5.9",
    "@types/bcrypt": "^5.0.0",
    "bcrypt": "^5.1.0",
    "bytemd": "^1.21.0",
    "h3-formidable": "^0.3.6",
    "juejin-markdown-themes": "^1.29.7",
    "pinia": "^2.1.4",
    "remark-directive": "^2.0.1",
    "vue3-toastify": "^0.1.11"
  }
}
