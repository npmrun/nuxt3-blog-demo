{
	"name": "nuxt-app",
	"type": "module",
	"private": true,
	"scripts": {
		"clean": "rimraf .output",
		"build": "npm-run-all -s clean nu:build deploy",
		"dev": "pnpm nu:dev",
		"nu:dev": "nuxt dev",
		"nu:build": "nuxt build",
		"nu:prepare": "nuxt prepare",
		"nu:preview": "nuxt preview",
		"nu:generate": "nuxt generate",
		"migrate": "prisma migrate dev --name",
		"deploy": "node scripts/create-deploy.mjs",
		"db:gui": "prisma studio",
		"db:generate": "prisma generate",
		"postinstall": " npm-run-all -s nu:prepare db:generate",
		"start": "node .output/server/index.mjs",
		"deps": "npx taze major",
		"lint:js": "eslint --ext \".ts,.vue\" --ignore-path .gitignore .",
		"lint:prettier": "prettier --check .",
		"lint": "pnpm lint:js && pnpm lint:prettier",
		"lintfix": "prettier --write --list-different . && pnpm lint:js --fix",
		"e2e:test": "cypress run",
		"test": "vitest run"
	},
	"devDependencies": {
		"@nuxt/devtools": "latest",
		"@nuxt/test-utils": "^3.13.1",
		"@nuxtjs/color-mode": "^3.4.1",
